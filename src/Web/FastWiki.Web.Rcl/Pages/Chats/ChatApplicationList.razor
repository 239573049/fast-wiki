<MList Nav
       Dense>
    <MListItemGroup @bind-Value="SelectedItem"
                    Color="primary">
        @if (ChatApplication?.Result != null)
        {
            @foreach (var item in ChatApplication.Result)
            {
                <MListItem Value="item.Id">
                    <MListItemContent>
                        <MListItemTitle>@item.Name</MListItemTitle>
                    </MListItemContent>
                </MListItem>
            }
        }
    </MListItemGroup>
</MList>